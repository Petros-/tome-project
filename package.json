{
  "name": "tome-project",
  "version": "1.0.0",
  "description": "An application for artists to store, download, and organize their artworks.",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "npm run start --prefix server",
    "client": "npm run dev --prefix client",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:client": "npm run test --prefix client",
    "test:server": "npm run test --prefix server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Petros-/tome-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Petros-/tome-project/issues"
  },
  "homepage": "https://github.com/Petros-/tome-project#readme",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
